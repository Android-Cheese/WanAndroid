<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="120dp"
    android:layout_height="wrap_content"
    android:layout_gravity="right"
    android:layout_marginTop="25dp"
    android:padding="@dimen/dp_4"
    android:background="@drawable/bg_solid_round_color_white_radius_def"
    android:orientation="vertical">

    <TextView
        android:id="@+id/dialog_web_menu_tv_collect"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:background="@drawable/anylayer_common_bg_press_main"
        android:gravity="center"
        android:text="收藏"
        android:textColor="@color/white"
        android:textSize="@dimen/text_content" />

    <TextView
        android:id="@+id/dialog_web_menu_tv_share"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:gravity="center"
        android:text="分享"
        android:background="@drawable/anylayer_common_bg_press_main"
        android:textColor="@color/white"
        android:textSize="@dimen/text_content" />

    <TextView
        android:id="@+id/dialog_web_menu_tv_browser"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:gravity="center"
        android:text="浏览器打开"
        android:background="@drawable/anylayer_common_bg_press_main"
        android:textColor="@color/white"
        android:textSize="@dimen/text_content" />

</LinearLayout>